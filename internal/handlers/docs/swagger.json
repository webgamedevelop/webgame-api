                    "type": "string"
                },
                "deletedAt": {
                    "$ref": "#/definitions/gorm.DeletedAt"
                },
                "dockerEmail": {
                    "type": "string",
                    "maxLength": 100
                },
                "dockerPassword": {
                    "type": "string",
                    "maxLength": 100
                },
                "dockerServer": {
                    "type": "string",
                    "maxLength": 100
                },
                "dockerUsername": {
                    "type": "string",
                    "maxLength": 50
