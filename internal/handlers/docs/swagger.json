            "type": "object",
            "properties": {
                "time": {
                    "type": "string"
                },
                "valid": {
                    "description": "Valid is true if Time is not NULL",
                    "type": "boolean"
                }
            }
        },
        "models.ImagePullSecret": {
            "type": "object",
            "required": [
                "dockerPassword",
                "dockerServer",
                "dockerUsername",
                "name",
                "secretName",
                "secretNamespace"
